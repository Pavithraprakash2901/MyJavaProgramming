import java.util.*;
class TestClass
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        String str = sc.nextLine();
        if(str.contains("HH"))
        {
            System.out.println("NO");
        }
        else
        {
            str.replace(".","B");
            System.out.println("YES");
            System.out.println(str);
        }
        
    }
}
