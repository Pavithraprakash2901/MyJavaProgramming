import java.util.*;
class Main{
   public static void main(String[] args){
   Scanner sc = new Scanner(System.in);
   int row,col,num,i,j,k;
   row = sc.nextInt();
   col=sc.nextInt();
   for(i=0;i<row;i++,System.out.print("\n"))
   {
     num=row;
     for(j=1;j<=i;j++)
     {
       System.out.print(num);
       num--;
     }
     for(k=1;k<=col-i;k++)
     {
       System.out.print(num);
     }
   }
}
}


// ip= 5 5

