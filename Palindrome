import java.util.*;
class Main 
{

	public static void main(String[] args) 
        {
		Scanner obj = new Scanner(System.in);
		int num = obj.nextInt();
		int rem,rev=0,temp;
                temp=num;
                while(num!=0)
		{
                   rem=num%10;
                   rev=rev*10+rem;
                  num =  num/10;
                }
                   System.out.println(rev);

                if(temp==rev)
                   System.out.println("Palindrome");     
                else
                   System.out.println("Not Palindrome");
        }
}
